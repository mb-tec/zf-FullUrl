{
  "name": "mb-tec/zf-fullurl",
  "description": "Url Plugin for Zend Framework 3",
  "homepage": "https://github.com/mb-tec/zf-FullUrl",
  "type": "library",
  "license": "GPL-3.0",
  "keywords": [
    "mb-tec",
    "url",
    "zf3",
    "zend",
    "framework"
  ],
  "authors": [
    {
      "name": "Matthias BÃ¼sing",
      "email": "info@mb-tec.eu",
      "homepage": "http://www.mb-tec.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-uri": "^2.5",
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-mvc": "^3.0",
    "zendframework/zend-router": "^3.0",
    "zendframework/zend-coding-standard": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.7",
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.0",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-stdlib": "^3.1"
  },
  "suggest": {
    "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
  },
  "autoload": {
    "psr-0": {
      "MBtecZfFullUrl\\": "./src/"
    },
    "psr-4": {
      "MBtecZfFullUrl\\": "./src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MBtecZfFullUrl\\Test\\": "./test/"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v"
  }
}
