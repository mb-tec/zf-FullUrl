{
  "name": "mb-tec/zf-fullurl",
  "description": "Url Plugin for Zend Framework 3",
  "homepage": "https://github.com/mb-tec/zf-FullUrl",
  "type": "library",
  "license": "GPL-2.0",
  "keywords": [
    "mb-tec",
    "url",
    "zf3",
    "zend",
    "framework"
  ],
  "authors": [
    {
      "name": "Matthias BÃ¼sing",
      "email": "info@mb-tec.eu",
      "homepage": "http://www.mb-tec.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-uri": "^2.5",
    "zendframework/zend-mvc": "^3.0",
    "zendframework/zend-view": "^2.8",
    "zendframework/zend-router": "^3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.7",
    "zendframework/zend-coding-standard": "^1.0",
    "phpunit/phpunit": "^5.7",
    "phpmd/phpmd": "^2.5",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^3.0"
  },
  "suggest": {
    "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
  },
  "autoload": {
    "psr-0": {
      "MBtecZfFullUrl\\": "./src/"
    },
    "psr-4": {
      "MBtecZfFullUrl\\": "./src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MBtecZfFullUrl\\Test\\": "./test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "phpcpd": "phpcpd",
    "phpmd": "phpmd",
    "phploc": "phploc src",
    "phpunit": "phpunit --colors=always"
  }
}
